.here _main
    ldi r15 5
    ldi r0 $file
    ldi r1 1
    syscall

    ldi r15 3
    ldi r0 $file_content
    syscall
    
    pop r0
    ldi r15 8
    ldi r1 1000
    ldi r2 $file_content
    syscall

    ldi r15 1
    ldi r0 0
    syscall

.here file
    .asciiz "test.txt"

.here file_content
    .asciiz "Hello, world!\n"
